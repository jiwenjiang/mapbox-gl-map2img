{
  "name": "Map2img",
  "version": "1.0.0",
  "description": "Produce universal library with webpack and es6",
  "main": "lib/map2img.min.js",
  "scripts": {
    "build": "webpack --env dev && webpack --env build",
    "dev": "webpack-dev-server --progress --colors --watch --env dev --host 127.0.0.1 --mode development --content-base ./demo",
    "test": "mocha --require babel-register --colors ./test/*.spec.js",
    "test:watch": "mocha --require babel-register --colors -w ./test/*.spec.js"
  },
  "repository": {
    "type": "git",
    "url": "https://github.com/jiwenjiang/mapbox-gl-map2img"
  },
  "keywords": [
    "mapbox-gl-download"
  ],
  "author": "j_bleach",
  "license": "BSD 2-Clause License",
  "bugs": {
    "url": "https://github.com/jiwenjiang/mapbox-gl-map2img/issues"
  },
  "devDependencies": {
    "@babel/cli": "^7.0.0-beta.51",
    "@babel/core": "^7.0.0-beta.51",
    "@babel/plugin-proposal-class-properties": "^7.1.0",
    "@babel/plugin-proposal-object-rest-spread": "^7.0.0",
    "@babel/plugin-proposal-decorators": "^7.4.4",
    "@babel/preset-env": "^7.0.0-beta.51",
    "babel-eslint": "^8.0.3",
    "babel-loader": "^8.0.0-beta.4",
    "babel-plugin-add-module-exports": "^0.2.1",
    "babel-preset-env": "^7.0.0-beta.3",
    "babel-register": "^7.0.0-beta.3",
    "chai": "^4.1.2",
    "eslint": "^5.0.1",
    "eslint-loader": "^2.0.0",
    "jsdom": "16.5.0",
    "jsdom-global": "3.0.2",
    "mocha": "^4.0.1",
    "uglifyjs-webpack-plugin": "^1.2.7",
    "webpack": "^4.29.6",
    "webpack-cli": "^3.3.0",
    "webpack-dev-server": "^3.2.1",
    "yargs": "^10.0.3"
  },
  "dependencies": {
    "@mapbox/mapbox-gl-directions": "^4.0.2"
  }
}
